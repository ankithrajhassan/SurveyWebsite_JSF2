<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

  <managed-bean>
    <managed-bean-name>studentDetails</managed-bean-name>
    <managed-bean-class>
      assignment2.StudentDetails
    </managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  
 <navigation-rule> 
 
     <from-view-id>/SurveyForm.xhtml</from-view-id> 
     
     <navigation-case>
       <from-outcome>thankyou</from-outcome>
       <to-view-id>/Thanks.xhtml</to-view-id> 
     </navigation-case>
     <navigation-case>
       <from-outcome>winner</from-outcome>
        <to-view-id>/Winner.xhtml</to-view-id> 
     </navigation-case>
     <navigation-case>
       <from-outcome>display</from-outcome>
        <to-view-id>/display.xhtml</to-view-id> 
     </navigation-case>
</navigation-rule>
  
  
</faces-config>
